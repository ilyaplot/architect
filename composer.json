{
  "name": "ilyaplot/architect",
  "description": "Architect is a static code analysis tool that helps to enforce rules for dependencies between software layers.",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilya Plotnikov",
      "email": "ilyaplot@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "nikic/php-parser": "^4.17",
    "symfony/config": "^6.3",
    "symfony/console": "^6.3",
    "symfony/dependency-injection": "^6.3",
    "symfony/event-dispatcher": "^6.3",
    "symfony/filesystem": "^6.3",
    "symfony/finder": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Ilyaplot\\Architect\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ilyaplot\\Architect\\Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3",
    "vimeo/psalm": "^5.15"
  }
}
